<%- include('header'); -%>
<h1>Hùng Cơm Chưa</h1>
<% config.forEach(function(quan){
    let tenQuan = quan.name;
    let valueQuan = quan.value;
    let _class = 'com_image';
    if (valueQuan !== 'com') {
        _class = 'bun_image';
    } %>
    <h1 style="color: red">AE ĐI LẤY CƠM THÌ MANG XUỐNG B1 RỒI ĐI THANG MÁY LÊN NHA! (TOÀ NHÀ YÊU CẦU THẾ)</h1>
    <h2><%= tenQuan %></h2>
    <div class="container_mon_an">
        <% quan.images.forEach(function(image){ %>
            <img class="<%= _class %>" src="/images/<%= valueQuan %>/<%= image %>" alt="com">
        <% }); %>
    </div>
<% }); %>
